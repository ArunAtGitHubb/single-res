<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <permission
        android:name="${applicationId}.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VegiNew"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activites.WebActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".activites.CartActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".activites.MapActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:exported="true"
            android:name=".activites.SpleshActivity"
            android:screenOrientation="fullSensor"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activites.OrderDetailActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".activites.NotificationActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".activites.AddAddressActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".activites.ComplainActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".activites.WishlistActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".activites.PaymentSummaryActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".activites.MyOrdersActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".activites.DeliveryOptionsActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".activites.FaqActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".activites.DeliveryAddressActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".activites.RatingAndReivewActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".activites.ProductDetailActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".activites.SearchActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".activites.MainActivity"
            android:screenOrientation="fullSensor" />

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />


        <activity
            android:name="cat.ereza.customactivityoncrash.activity.DefaultErrorActivity"
            android:process=":error_activity"
            android:theme="@style/Theme.VegiNew" />


        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/map_key" />
    </application>

</manifest>